<%
qtools.validateProperties({
	targetObject:this,
	propList:[
		{name:'displayParameters'},
		{name:'viewHelper'},
		{name:'schoolList'}
	],
 source:'session.register.mainPanel' });

qtools.validateProperties({
	targetObject:displayParameters,
	propList:[
		{name:'saveButton'},
		{name:'loginButton'},
		{name:'status'},
	],
 source:'session.register.mainPanel' });

var optionString;

optionString=viewHelper.makeSelectTag({
	selectVarName:'schoolRefId',
	sourceObj:this.schoolList,
	valuePropertyName:'refId',
	labelPropertyName:'name',
	firstItemLabel:'Select School',
	firstItemValue:'',
	selectClassName:'schoolIdClassString' //this is for debug purposes only, see register.initDomObjects()

});

var password='tqwhite'+Math.floor((Math.random()*1000)+1);
%>
<div class='entryBox' style='height:370px;width:300px;'>

<div class='entryPanelErrorList' style='margin-top:25px;height:30px;width:100%' id='<%==displayParameters.status.divId%>'>Welcome!</div>

<table style='border:none;font-size:10pt;'>
	<tr><td style='width:100px;'>First Name:</td><td><input prompt='required' name='firstName' value='TQ'></td></tr>
	<tr><td style='width:100px;'>Last Name:</td><td><input prompt='required' name='lastName' value='White II'></td></tr>
	<tr><td style='width:100px;'>User Name:</td><td><input prompt='required' name='userName' value='<%=password%>'></td></tr>
	<tr><td>Password:</td><td><input prompt='6+ characters' name='password' value='123456'></td></tr>
	<tr><td>Email Address:</td><td><input prompt='required' name='emailAdr' value='tq@justkidding.com'></td></tr>
	<tr><td>Confirm Email:</td><td><input prompt='required' name='confirmEmail' value='tq@justkidding.com'></td></tr>
	<tr><td>Phone:</td><td><input prompt='required' name='phone'  value='000-000-0000'></td></tr>
	<tr><td>School:</td><td><%== optionString %></td></tr>
</table>
<div class='basicButton' style='margin-left:136px;margin-top:20px;' id='<%==displayParameters.saveButton.divId%>'></div>

<div class='smallButton' style='margin-left:165px;margin-top:20px;' id='<%==displayParameters.loginButton.divId%>'></div>
</div>