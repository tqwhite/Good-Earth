<%
qtools.validateProperties({
	targetObject:this,
	propList:[
		{name:'displayParameters'},
		{name:'viewHelper'},
		{name:'formData'},
		{name:'adminFlag'}
	],
 source:'session.dashboard.dashboardMainX' });

qtools.validateProperties({
	targetObject:displayParameters,
	propList:[
		{name:'myId'},
		{name:'accountSpace'},
		{name:'kidSpace'},
		{name:'purchaseSpace'}

	],
 source:'session.dashboard.dashboardMainY' });
 if (this.adminFlag){
 var message = "<div id='"+ displayParameters.instructions.divId+"' style='font-size:10pt;position:relative;color:red;font-weight:bold;margin:10px 50px;'>Schools are always open for Admin testing.</div>"
}
else{
 var message = "<div id='"+ displayParameters.instructions.divId+"' style='font-size:10pt;position:relative;color:red;margin:10px 50px;'>Click on the Student's name to change information.</div>"
}
%><div class='mainContainer' id='<%= displayParameters.myId.divId %>'>


	<div class='smallButton' style='position:absolute;top:14px;right:140px;width:40px;' id='<%=this.displayParameters.adminButton.divId%>'></div>
	<div class='accountSpace' id='<%= displayParameters.accountSpace.divId %>'></div>
	
	<%== message %>
	<div class='kidSpace' id='<%= displayParameters.kidSpace.divId %>'>
	</div>
	<div class='purchaseSpace' id='<%= displayParameters.purchaseSpace.divId %>'></div>

</div>